@model TimesheetDTO
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>
<form asp-controller="Timesheets" asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="UserId"></label><input asp-for="UserId" class="form-control" />
        <span asp-validation-for="UserId"></span>
    </div>
    <div class="form-group">
        <label asp-for="ProjectId"></label><input asp-for="ProjectId" class="form-control" />
        <span asp-validation-for="ProjectId"></span>
    </div>
    <input type="submit" value="Create" />
</form>
